							</VisualStateGroup>
						</VisualStateManager.VisualStateGroups>
						<Grid x:Name="Background" Background="{TemplateBinding Background}" UseLayoutRounding="False" d:LayoutOverrides="Width, Height" RenderTransformOrigin="0.5,0.5">
							<Grid.RenderTransform>
